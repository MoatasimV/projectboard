{
  "name": "server",
  "version": "1.0.0",
  "author": "Vishwajeet Raj",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/src/app.js",
    "server": "nodemon server/src/app.js",
    "start:prod": "node server/src/app.js",
    "client": "yarn start --prefix client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "yarn install && yarn --cwd ./client install && yarn --cwd ./client run build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.6.0",
    "jwks-rsa": "^2.0.4",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.13.5",
    "morgan": "^1.10.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
  },
  "engines": {
    "node": "<14.0.0"
  }
}
